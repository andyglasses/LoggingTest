{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SerilogHttpSinkURL": "",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        // Namespaces
        "Microsoft": "Warning",
        // Necessary for VS Code to automatically launch browser, see https://stackoverflow.com/a/71781505/1431147
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "HealthChecks": "Warning",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information"
      }
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  }
}
